{
  "name": "YaMapTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "start:clean": "watchman watch-del-all && yarn start cache clean",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write \"./**/*.{ts,tsx}\"",
    "husky": "yarn pretty-quick --staged && npx lint-staged && yarn tsc",
    "create-toolkit-dir": "./scripts/createToolkitDir.sh",
    "prepare": "sh ./scripts/createHuskyConfig.sh",
    "postinstall": "npx pod-install && patch-package",
    "pod-install": "RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "android:assembleRelease": "cd ./android && ./gradlew clean && ./gradlew assembleRelease && open ./app/build/outputs/apk",
    "android:bundleRelease": "cd ./android && ./gradlew clean && ./gradlew bundleRelease && open ./app/build/outputs/bundle"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.3",
    "eslint-config-prettier": "^8.8.0",
    "immer": "^9.0.21",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-config": "^1.5.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-text-input-mask": "^3.1.4",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.7"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.5.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^18.0.31",
    "@types/react-native": "^0.71.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lodash.debounce": "^4.0.8",
    "metro-react-native-babel-preset": "0.76.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
